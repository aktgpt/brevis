{
  "gpu_device":"2,3",
  "exp_folder": "exps",
  "run_mode": "train",
  "test": {
    "tester": "BaseTester",
    "model_path": "model_best_loss.pth"
  },
  "train": {
    "trainer": "BaseTrainer",
    "save_name": "split3",
    "resume_training": false,
    "model_resume_name": "resume_model_epoch_500.pth",
    "gradient_accumulation":false,
    "epochs": 5000,
    "save_interval": 100,
    "early_stop": 10,
    "optimizer": {
      "type": "AdamW",
      "args": {
        "lr": 0.0001
      }
    },
    "lr_scheduler": false
  },
  "losses": [
    {
      "weight": 1.0,
      "type": "L1Loss",
      "args": null
    }
  ],
  "models": [
    {
      "type": "DenseUNet",
      "args": {
        "in_channels": 7,
        "nb_classes": 1,
        "init_conv_size": 3,
        "init_conv_filters": 48,
        "init_conv_stride": 1,
        "down_blocks": [
          4,
          4,
          4,
          4
        ],
        "bottleneck_layers": 4,
        "up_blocks": [
          4,
          4,
          4,
          4
        ],
        "growth_rate": 12,
        "compression": 1.0,
        "dropout_rate": 0.2,
        "upsampling_type": "pixelshuffle_blur",
        "early_transition": false,
        "transition_pooling": "max",
        "batch_norm": "batchnorm",
        "include_top": true,
        "efficient": false
      }
    }
  ],
  "data": {
    "dataset": "FastBaseDataset",
    "folder": "/scratch-shared/phil/AZ_competition",
    "train_csv_file": "exp_stats/ablation_train_split_3.csv",
    "valid_csv_file": "exp_stats/ablation_valid_split_3.csv",
    "test_csv_file": "exp_stats/final_test_all.csv",
    "magnification": "60x",
    "output_channel": "C3",
    "batch_size": 4,
    "shuffle": true,
    "standardize": false,
    "normalize": true,
    "augmentations": [
    {
        "type": "RandomCrop",
        "args": {
            "height": 512,
            "width": 512,
            "always_apply": true,
            "p": 1.0
        }
        },
      {
        "type": "HorizontalFlip",
        "args": {
          "p": 0.5
        }
      },
      {
        "type": "VerticalFlip",
        "args": {
          "p": 0.5
        }
      },
      {
        "type": "RandomRotate90",
        "args": {
          "p": 0.5
        }
      }
    ]
  }
}